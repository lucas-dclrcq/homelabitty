{
  "sources": [
    {
      "type": "endpointlz",
      "enable": true,
      "name": "navidromeLucas",
      "data": {
        "token": "${SCROBBLE_TOKEN}"
      }
    },
    {
      "enable": true,
      "name": "MPDMusicPlayer",
      "data": {
        "url": "music.hoohoot.xyz:6600"
      },
      "options": {
        "disableDiscovery": false
      },
      "type": "mpd"
    }
  ],
  "clients": [
    {
      "type": "listenbrainz",
      "name": "listenbrainz",
      "enable": true,
      "configureAs": "client",
      "data": {
        "token": "{{ .LZ_TOKEN }}",
        "username": "{{ .LZ_USER }}"
      },
      "options": {
        "nowPlaying": true
      }
    },
    {
      "type": "listenbrainz",
      "name": "the-elder-scrobz",
      "enable": true,
      "configureAs": "client",
      "data": {
        "token": "${SCROBBLE_TOKEN}",
        "username": "{{ .LZ_USER }}",
        "url": "http://the-elder-scrobz.media.svc.cluster.local:3000/api/v1/1/"
      },
      "options": {
        "nowPlaying": true
      }
    },
    {
      "type": "lastfm",
      "name": "lastfm",
      "enable": true,
      "configureAs": "client",
      "data": {
        "apiKey": "{{ .LASTFM_API_KEY }}",
        "secret": "{{ .LASTFM_SECRET }}",
        "redirectUri": "https://multi-scrobbler.${LIMITED_DOMAIN}/lastfm/callback"
      },
      "options": {
        "nowPlaying": true
      }
    }
  ]
}
